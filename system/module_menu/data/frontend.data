frontend
- frontend_block_tree
    display
      check: dpath
      where: block
      match: %^trees/.*$%
    styles
    - tree_style
        file: frontend/tree.cssd
        media: all
- frontend_block_tabs
    display
      check: dpath
      where: block
      match: %^tabs/.*$%
    styles
    - tabs_style
        file: frontend/tabs.cssd
        media: all